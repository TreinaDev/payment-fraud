require 'rails_helper'

feature 'Collaborator sign in' do
  scenario 'successfuly' do 
    admin = User.create!(email: 'ivan@smartflix.com.br', password: '123456')

    visit root_path
    within('form') do 
    fill_in 'E-mail', with: User.email
    fill_in 'Senha', with: '123456'
    click_on 'Entrar'
    end
  end

end